    "icon": "./assets/images/yason.png",
    "scheme": "myfreshapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": { 
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
