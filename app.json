      },
      "edgeToEdgeEnabled": false,
      "package": "com.abeniexpo.MyFreshApp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/yason.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "https://yasonbackend.yasonsc.com",
      "router": {},
      "eas": {
        "projectId": "e389b01d-846b-4f8e-a649-e38d0dedda06"
